<p id="notice"><%= notice %></p>

<h1>Consultations</h1>

<table>
  <thead>
    <tr>
      <th>Disease</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Body mass</th>
      <th>Temperature</th>
      <th>Respiratory rate</th>
      <th>Systolic</th>
      <th>Diastolic</th>
      <th>Heart rate</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consultations.each do |consultation| %>
      <tr>
        <td><%= consultation.disease %></td>
        <td><%= consultation.height %></td>
        <td><%= consultation.weight %></td>
        <td><%= consultation.body_mass %></td>
        <td><%= consultation.temperature %></td>
        <td><%= consultation.respiratory_rate %></td>
        <td><%= consultation.systolic %></td>
        <td><%= consultation.diastolic %></td>
        <td><%= consultation.heart_rate %></td>
        <td><%= consultation.user_id %></td>
        <td><%= link_to 'Show', consultation %></td>
        <td><%= link_to 'Edit', edit_consultation_path(consultation) %></td>
        <td><%= link_to 'Destroy', consultation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consultation', new_consultation_path %>
